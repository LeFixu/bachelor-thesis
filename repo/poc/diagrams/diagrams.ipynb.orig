    "             ,color_discrete_sequence=my_colors,\n",
    "labels={'gender': 'Geschlecht'})\n",
    "fig.update_layout(\n",
    "        title='Zitate gruppiert nach Geschlecht und Nachrichtenportal',\n",
    "        title_x=0.5,\n",
    "        yaxis_title_text='Anzahl Zitate',\n",
    "        xaxis_title_text='Nachrichtenportal',\n",
    "        font=dict(\n",
    "                size=17\n",
    "        )\n",
    ")\n",
    "fig.show()"
